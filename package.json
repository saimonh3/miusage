{
  "name": "miusage",
  "description": "A simple plugin for Awesome Motive",
  "version": "1.0.0",
  "devDependencies": {
    "@wordpress/scripts": "^13.0.3"
  },
  "scripts": {
    "dev": "wp-scripts start assets/src/js/main.js --output-path=assets/build",
    "build": "wp-scripts build assets/src/js/main.js --output-path=assets/build",
    "lint:js": "wp-scripts lint-js assets/src/",
    "lint:css": "wp-scripts lint-style assets/src/",
    "format:js": "wp-scripts format-js assets/src/",
    "make-pot": "wp i18n make-pot ./ languages/miusage.pot --exclude=assets"
  }
}
